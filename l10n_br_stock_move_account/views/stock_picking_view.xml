<openerp>
    <data>

        <record model="ir.ui.view" id="stock_picking_moves_account_form">
            <field name="name">stock.picking.moves.account.form</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr='//page[@string="Additional Info"]' position="after">
                    <page string="Recebimento Provisório">
                        <separator string="Recebimento Provisório Contabilidade" />
                        <group>
                            <field name="temporary_received_journal_id" />
                        </group>
                        <field name="temporary_received_ids" />
                    </page>
                </xpath>
            </field>
        </record>

    </data>
</openerp>
